{
  "name": "daigakudatabase",
  "author": "Haruki Jay Shimada",
  "version": "0.3.0",
  "license": "MIT",
  "description": "Monorepo for DaigakuDatabase.com",
  "repository": {
    "url": "https://github.com/DrPoppyseed/daigakudatabase-client",
    "type": "git"
  },
  "scripts": {
    "start": "pnpm recursive run start",
    "test": "pnpm recursive run test",
    "lint": "pnpm recursive run lint",
    "lint:fix": "pnpm recursive run lint:fix",
    "format": "pnpm recursive run format",
    "build": "pnpm recursive run build",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "packages/*.{js,jsx,ts,tsx}": [
      "pnpm run format",
      "pnpm run lint:fix"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^3.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.7",
    "pinst": "^3.0.0",
    "prettier": "^2.6.0",
    "typescript": "^4.6.3"
  }
}
