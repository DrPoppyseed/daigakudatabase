{
  "name": "daigakudatabase-client",
  "author": "Haruki Jay Shimada <peaske16180@gmail.com> (https://github.com/DrPoppyseed)",
  "version": "0.2.1",
  "license": "MIT",
  "description": "⛏ Active development branch of DaigakuDatabase.com",
  "repository": {
    "url": "https://github.com/DrPoppyseed/daigakudatabase-client",
    "type": "git"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest --watchAll=false",
    "test:watch": "jest",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{ js,jsx,ts,tsx }'",
    "lint:fix": "eslint --fix 'src/**/*.{ js,jsx,ts,tsx }'",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "postinstall": "husky install",
    "analyze": "ANALYZE=true yarn build",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm run lint:fix",
      "pnpm run format"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@firebase/app": "^0.7.20",
    "@firebase/app-types": "^0.7.0",
    "@hookform/resolvers": "^2.8.8",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "@mui/styles": "^5.5.1",
    "@mui/system": "^5.5.2",
    "@reduxjs/toolkit": "^1.8.0",
    "@rollup/plugin-image": "^2.1.1",
    "@types/d3": "^7.1.0",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.23",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "d3": "^7.3.0",
    "firebase": "^9.6.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.28.1",
    "react-intl": "^5.24.7",
    "react-is": "^17.0.2",
    "react-loader-spinner": "^6.0.0-0",
    "react-query": "^3.34.17",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "vite": "^2.8.6",
    "vite-plugin-checker": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^3.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "pinst": "^3.0.0",
    "prettier": "^2.6.0",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.70.1",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.3"
  }
}
